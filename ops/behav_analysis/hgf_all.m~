function hgf_all(default_config)

fol_name = 'data/behav_analyzed_perf';
all = dir(strcat(fol_name,'/*.mat'));
nfailed = 0;

for k = 1:length(all)
   try
    single_subject_HGF_analysis(fol_name,all(k).name,default_config);
   catch
       nfailed = nfailed + 1;
       disp('-------------------------------------------------------------')
       disp('-------------------------------------------------------------')
       disp('-------------------------------------------------------------')
       continue
   end
end
fprintf('\n
end